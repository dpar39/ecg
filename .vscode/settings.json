// Place your settings in this file to overwrite default and user settings.
{
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 2000,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "./thirdparty/android-ndk-r15c": true,
        "./thirdparty/sdk-tools-*": true,
        "./thirdparty/gradle-*": true,
        "./thirdparty/emsdk/*": true,
        "./thirdparty/install_emscripten": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "./thirdparty/curl-*": true,
        "./thirdparty/curlpp-*": true,
        "./thirdparty/dlib-*": true,
        "./thirdparty/opencv-*": true,
        "./thirdparty/opencv_contrib-*": true,
        "./thirdparty/SDL2-*": true,
        "./thirdparty/googletest-*": true,
        "./thirdparty/rapidjson-*": true
    },
    "files.trimTrailingWhitespace": true,
    "editor.codeLens": false,
    "editor.autoIndent": "brackets",
    "python.linting.pycodestyleEnabled": true,
    "python.linting.pycodestyleArgs": [
        "--ignore=E501",
        "--max-line-length=100"
    ],
    "prettier.singleQuote": true,
    "prettier.printWidth": 100,
    "cSpell.words": [
        "Rodrigues",
        "elmt",
        "emscripten",
        "exif",
        "greenyellow",
        "isdir",
        "onend",
        "onmove",
        "onstart",
        "opencv",
        "wasm",
        "webapp"
    ],
    "testMate.cpp.test.executables": "build_*_debug_x64/**/yaiat_test*",
    "testMate.cpp.test.workingDirectory": "${absDirpath}",
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Args": [
        "--max-line-length=110"
    ],
    "clangd.arguments": [
        "--clang-tidy",
        "--background-index",
        "--suggest-missing-includes",
        "--compile-commands-dir=build_windows_release_x64"
    ],
    "workbench.colorCustomizations": {
        "titleBar.activeBackground": "#304d68",
    },
    "terminal.integrated.shell.windows": "cmd.exe",
    "terminal.integrated.shellArgs.windows": [
        "/K",
        "C:\\Cmder\\vendor\\init.bat"
    ],
    "terminal.integrated.automationShell.windows": "cmd.exe",
    "files.associations": {
